{
  "name": "race-shifting-idea",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "deploy": "aws s3 rm s3://race-shifting-idea.calacademy.org --recursive && aws s3 cp build s3://race-shifting-idea.calacademy.org --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "fetch-jsonp": "^1.1.3",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  },
  "homepage": "/race-shifting-idea.calacademy.org"
}
